<?xml version='1.0' encoding='UTF-8' ?>
<svg xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' viewBox='{{min_x}} {{min_y}} {{max_x}} {{max_y e}}'>
  <defs>
    <style type='text/css'><![CDATA[
      line, polyline, polygon, path, rect, circle {
        fill: none;
        stroke: #000000;
        stroke-linecap: round;
        stroke-linejoin: round;
        stroke-miterlimit: 10.00;
      }
      .precip-bar {
        stroke-width: 1.07; 
        stroke: none; 
        stroke-linecap: butt; 
        fill: #F8766D;
      }
    
      .streamflow-bar {
        stroke-width: 1.07;
        stroke: none; 
        stroke-linecap: butt; 
        fill: #619CFF;
      }
    
      .evapotranspiration-bar {
        stroke-width: 1.07;
        stroke: none;
        stroke-linecap: butt;
        fill: #00BA38;
      }
    
      .axis-label {
        font-size: 8.80px;
        fill: #4D4D4D;
        font-family: Arial;
      }
    
      .axis-tick {
        stroke-width: 1.07;
        stroke: #333333;
        stroke-linecap: butt;
      }
    
      .axis-label {
        font-size: 8.80px;
        fill: #4D4D4D;
        font-family: Arial;
      }
    
      .axis-units-label {
        font-size: 11.00px;
        font-family: Arial;
      }
    
      .legend-text {
        font-size: 8.80px;
        font-family: Arial;
      }
    
    ]]></style>
  </defs>
<!-- Main rectangles -->
<rect x='{{p_rect_x}}' y='{{p_rect_y}}' width='{{rect_w}}' height='{{p_rect_h}}' class='precip-bar'/>
<rect x='{{ueq_rect_x}}' y='{{u_rect_y}}' width='{{rect_w}}' height='{{u_rect_h}}' class='unknown-bar'/>
<rect x='{{ueq_rect_x}}' y='{{q_rect_y}}' width='{{rect_w}}' height='{{q_rect_h}}' class='streamflow-bar'/>
<rect x='{{ueq_rect_x}}' y='{{e_rect_y}}' width='{{rect_w}}' height='{{e_rect_h}}' class='evapotranspiration-bar'/>

<!-- y axis text -->
<g><text x='{{y_tick_text_x}}' y='{{bottom_tick_y}}' class="axis-label" lengthAdjust='spacingAndGlyphs'>{{bottom_tick}}</text></g>
<g><text x='{{y_tick_text_x}}' y='{{top_tick_y}}' class="axis-label" lengthAdjust='spacingAndGlyphs'>{{top_tick}}</text></g>

<!-- x/y axis ticks 
<polyline points='31,162 34,162 ' class='axis-tick' />
<polyline points='31,79 34,79 ' class='axis-tick' />
<polyline points='70,261 70,258' class='axis-tick' />
<polyline points='131,261 131,258 ' class='axis-tick' /> -->

<!-- x axis text 
<g><text x='66.39' y='269.17' class="axis-label" textLength='7.34px' lengthAdjust='spacingAndGlyphs'>In</text></g>
<g><text x='123.57' y='269.17' class="axis-label" textLength='14.18px' lengthAdjust='spacingAndGlyphs'>Out</text></g>
<g><text transform='translate(13.35,147.61) rotate(-90)' class="axis-units-label" textLength='31.80px' lengthAdjust='spacingAndGlyphs'>inches</text></g> -->

<!-- legend rectangles -->
<rect x='184.74' y='108.66' width='15.86' height='15.86' class='precip-bar' />
<rect x='184.74' y='125.94' width='15.86' height='15.86' class='evapotranspiration-bar' />
<rect x='184.74' y='143.22' width='15.86' height='15.86' class='streamflow-bar' />

<!-- legend text -->
<g><text x='203.47' y='119.74' class='legend-text' textLength='48.43px' lengthAdjust='spacingAndGlyphs'>Precipitation</text></g>
<g><text x='203.47' y='137.02' class='legend-text' textLength='73.38px' lengthAdjust='spacingAndGlyphs'>Evapotranspiration</text></g>
<g><text x='203.47' y='154.30' class='legend-text' textLength='44.01px' lengthAdjust='spacingAndGlyphs'>Streamflow</text></g>
</svg>
